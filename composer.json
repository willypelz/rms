{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "alchemy/zippy": "1.0.0",
        "algolia/algoliasearch-client-php": "^3.2",
        "arcanedev/log-viewer": "^8.0.0",
        "aws/aws-sdk-php": "~3.0",
        "barryvdh/laravel-dompdf": "^0.8.6",
        "barryvdh/laravel-snappy": "^0.4.0",
        "bumbummen99/shoppingcart": "4.0.0",
        "buzz/laravel-google-captcha": "2.*",
        "consoletvs/charts": "6.*",
        "doctrine/dbal": "^2.5",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "genealabs/laravel-mixpanel": "^0.11.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "h4cc/wkhtmltoimage-amd64": "0.12.x",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "ixudra/curl": "^6.1",
        "jdavidbakr/mail-tracker": "^5.0.3",
        "jenssegers/agent": "^2.5",
        "laravel/framework": "^8.0",
        "laravel/helpers": "^1.2",
        "laravel/horizon": "^5.0.0",
        "laravel/sanctum": "^2.11",
        "laravel/scout": "^9.3",
        "laravel/telescope": "^4.0",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.3",
        "laravelcollective/html": "^6.0",
        "maatwebsite/excel": "3.1.*",
        "madnest/madzipper": "1.0.*",
        "predis/predis": "^1.1",
        "seamlesshr/solrpackage": "dev-SolrUpgradeV8",
        "sentry/sentry-laravel": "^2.10",
        "solarium/solarium": "^5.1",
        "spatie/calendar-links": "^1.2",
        "trebol/entrust": "2.0",
        "webpatser/laravel-uuid": "^2.0",
        "wemersonjanuario/wkhtmltopdf-windows": "0.12.2.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "fzaninotto/faker": "^1.9.2",
        "filp/whoops": "~2.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
     "repositories": [
        {
            "type": "vcs",
            "url": "git@bitbucket.org:insidify/solr-package.git"

        }
    ],
    "autoload": {
        "classmap": [
            "database"
        ],
         "files": [
            "app/Helpers/util.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan clear-compiled",
            "@php artisan config:clear",
            "@php artisan horizon:publish --ansi",
            "@php artisan telescope:publish --ansi",
            "@php artisan log-viewer:publish --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "ext-pcntl": "7.3",
            "ext-posix": "7.3"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
