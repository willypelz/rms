{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3",
        "fideloper/proxy": "^4.2",
        "laravel/framework": "^8.0",
        "laravel/tinker": "^2.0",
        "fruitcake/laravel-cors": "^1.0",
        "bumbummen99/shoppingcart": "4.0.0",
        "laravelcollective/html": "^6.0",
        "doctrine/dbal": "^2.5",
        "guzzlehttp/guzzle": "^7.0.1",
        "ixudra/curl": "^6.1",
        "alchemy/zippy": "1.0.0",
        "maatwebsite/excel": "3.1.*",
        "barryvdh/laravel-dompdf": "^0.8.6",
        "madnest/madzipper": "1.0.*",
        "consoletvs/charts": "6.*",
        "jenssegers/agent": "^2.5",
        "buzz/laravel-google-captcha": "2.*",
        "aws/aws-sdk-php": "~3.0",
        "barryvdh/laravel-snappy": "^0.4.0",
        "webpatser/laravel-uuid": "^2.0",
        "trebol/entrust": "2.0",
        "spatie/calendar-links": "^1.2",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "h4cc/wkhtmltoimage-amd64": "0.12.x",
        "sentry/sentry-laravel": "^1.0",
        "wemersonjanuario/wkhtmltopdf-windows": "0.12.2.3",
        "laravel/helpers": "^1.2",
        "jdavidbakr/mail-tracker": "^5.0.3",
        "seamlesshr/solrpackage": "dev-SolrUpgradeV8",
        "solarium/solarium": "^5.1",
        "predis/predis": "^1.1",
        "laravel/horizon": "^5.0.0",
        "laravel/telescope": "^4.0",
        "laravel/ui": "^3.3",
        "genealabs/laravel-mixpanel": "^0.11.0",
        "arcanedev/log-viewer": "~5.1.7"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9.2",
        "mockery/mockery": "1.3.1",
        "facade/ignition": "^2.0",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "barryvdh/laravel-debugbar": "^3.2",
        "filp/whoops": "~2.0"
    },
     "repositories": [
        {
            "type": "vcs",
            "url": "https://git@bitbucket.org/insidify/solr-package.git"

        }
    ],
    "autoload": {
        "classmap": [
            "database"
        ],
         "files": [
            "app/Helpers/util.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan config:clear"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan config:clear"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "platform": {
            "ext-pcntl": "7.3",
            "ext-posix": "7.3"
        }
    }
}
